  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LeafTransformationScheme" eSuperTypes="#//TransformationScheme">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="A  LeafTransformationScheme is a TransformationScheme which does not contain any sub TransformationSchemes"/>
    </eAnnotations>
    <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
      <details key="name" value="LeafTransformationScheme"/>
      <details key="kind" value="elementOnly"/>
